<launch>

  <node pkg="turtlesim"
        type="turtlesim_node"
        name="simulator"/>
 
  <node pkg="turtlesim"
        type="turtle_teleop_key"
        name="keyboard"
        output="screen"/>

  <node pkg="super_kamnev_study_pkg"
        type="turtle_tf_broadcaster.py"
        name="turtle1_tf_broadcaster"
        output="screen">
      <remap from="input_pose" to="turtle1/pose"/>
      <param name="turtle_tf_name" value="turtle1"/>
  </node>


  <node pkg="rviz"
        type="rviz"
        name="rviz"/>

</launch>

