<launch>
  <arg name="new_topic_name" default="new_topic" doc="Имя топика для remap"/>
  
  <group ns="new_ns">
    <include file="$(find super_kamnev_study_pkg)/launch/another_one.launch">

      <arg name="topic_name" value="$(arg new_topic_name)"/>
    </include>

    <node name="sender" pkg="rospy_tutorials" type="talker.py" output="screen">
      <remap from="chatter" to="$(arg new_topic_name)"/>
    </node>

    <node name="receiver" pkg="rospy_tutorials" type="listener.py" output="screen">
      <remap from="chatter" to="$(arg new_topic_name)"/>
    </node>
    
  </group>
</launch>

